    "id_cols": ["TransactionID"],
    "timestamp": "TransactionDT",
    "target": "isFraud",
    "feature_order": "feature_order.txt"
  },
  "preprocessor": {"path": "preprocessor.pkl", "entry": "transform_one"},
  "models": {
    "lgbm": {"type": "lightgbm", "path": "model_lgbm.txt"},
